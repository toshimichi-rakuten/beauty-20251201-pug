extends ../../../../../_common/pug/Base/_layout.pug

block init
  -
    var parameter = '?v20250206'
    var meta = {
      title: '【楽天ビューティ×楽天銀行】最大2,000ポイントプレゼントキャンペーン',
      description: 'Rakuten BEAUTY x 楽天銀行 エントリー＆条件達成で最大2,000ポイントもらえる',
      url: 'https://beauty.rakuten.co.jp/cnt/topics/campaign/other/bank/',
      directory: 'bank', //- ディレクトリ名 for Click or Appear Tag（例：/xxxx/→'xxxx'）
      og_image: 'https://beauty.rakuten.co.jp/cnt/topics/campaign/other/bank/img/share.png' + parameter,//- default (シェア画像があれば変更)
    }
    var css = 'css/main.css' + parameter

block addHead
  //- nofollowが必要なときはコメントを外す
  //- meta(name='robots', content='noindex,nofollow')
  link(rel='stylesheet', href="css/splide.min.css")

block nav
  //- grobalNav / ''に入れた部分がcurrent表示になる
  +grobalNav('/cnt/topics/')

  //- breadcrumb
  -
    var lists = [
      {
        name: 'キャンペーン一覧・特集',
        link: 'https://beauty.rakuten.co.jp/cnt/topics/',
      },
      {
        name: meta.title, //- タイトル以外を設定したいときは変更
        link: 'current', //- 'current' is no link
      },
    ]
  +breadcrumb(lists)

block content
  //- #Pagename はページ固有の名前に置換してください
  #BankCampaign

    //- 終了対応
    #End
      - var endflag
      - var endTxt = 'このキャンペーンは終了しました' //- ページ上部の文言
      - var btnTxt = 'エントリー期間が終了しました' //- ボタンの文言
      +endSet(endTxt)

    //- Campaign Tag
    //- Rule: https://confluence.rakuten-it.com/confluence/pages/viewpage.action?pageId=2688949406
    #CpnTags
      -
        var taglist = {
          tag: [
            'エントリー必要', //- エントリー不要は自動的に青色になります
            //- '初回利用限定', //- 初回限定と全員、両方のクーポンがある場合はタグはなし
            '条件達成',
            'ネット予約・施術完了',
          ],
        }
      +campaignTag(taglist)

    //- ID、class名は自由
    section#Main.main(data-ratid='cpn_' + meta.directory + '_Main' data-ratevent='appear' data-ratparam='all')
      //- Use h1
      h1.main__title
        img.main__titleImg.-pc(src='img/main.png' + parameter, alt=meta.title)
        img.main__titleImg.-sp(src='img/main_sp.png' + parameter, alt=meta.title)
      .main__terms
        .main__termsBox
          p.main__termsTxt
            | キャンペーン期間：8月1日 0:00 〜 8月31日 23:59
          p.main__termsTxt
            | 条件達成期間：8月1日 0:00 〜 9月30日 23:59
      .main__terms__summary__wrap
        .main__terms__summary
          .main__terms__summary__step
            img.main__terms__summary__all.-pc(src='img/img_main_summary_all.png' + parameter, alt='楽天銀行口座をお持ちの方 1まずはエントリー 2楽天ビューティサロン予約※1 3施術&デビットカード決済※2 500ポイント' width='100%' height='auto')
            img.main__terms__summary__all.-sp(src='img/img_main_summary_all_sp.png' + parameter, alt='楽天銀行口座をお持ちの方 1まずはエントリー 2楽天ビューティサロン予約※1 3施術&デビットカード決済※2 500ポイント' width='100%' height='auto')
            img.main__terms__summary__new.-pc(src='img/img_main_summary_new.png' + parameter, alt='楽天銀行口座を初めて解説する方 1まだの方はエントリー 2楽天銀行口座解説&入金※3 1,500ポイント' width='100%' height='auto')
            img.main__terms__summary__new.-sp(src='img/img_main_summary_new_sp.png' + parameter, alt='楽天銀行口座を初めて解説する方 1まだの方はエントリー 2楽天銀行口座解説&入金※3 1,500ポイント' width='100%' height='auto')
          .main__terms__summary__point
            img.-pc(src='img/img_main_point.png' + parameter, alt='最大2,000ポイントGET' width='100%' height='auto')
            img.-sp(src='img/img_main_point_sp.png' + parameter, alt='最大2,000ポイントGET' width='100%' height='auto')
        p.main__terms__summary--info
          | ※1 
          span.main__terms__summary--info-bold
            | 3,500円（税込）以上のネット予約
          | 。※2 
          span.main__terms__summary--info-bold
            | 期間中合計3,500円（税込）以上の決済
          | （一部対象外のカードがあります）。楽天ビューティでの予約＆来店サロンで楽天銀行デビットカードでのお支払いに加え、対象のデビットカードが使えるお店でのお支払いもキャンペーン達成条件の対象となります。※3 本キャンペーンページ内
          span.main__terms__summary--info-bold
            | 「楽天銀行の口座を開設」ボタン経由の口座開設のみ
          | が対象。口座開設後に入金が必要。※本キャンペーンは予告なく変更・中止される可能性があります。※条件の達成順序は順不同です。※進呈するポイント（期間限定ポイント）には上限や条件があります。※キャンペーン詳細は
          a.main__termsLink(href='#Detail')
            | こちら

    //- menu
    section#Navigation.navigation(data-ratid='cpn_' + meta.directory + '_Navigation' data-ratevent='appear' data-ratparam='all')
