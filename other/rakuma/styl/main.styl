passStr = '../../../../../../'
@import passStr + '_common/stylus/Base/_init'

//------------------------------// Common - このページで使う共通設定。自由に設定可能

mainColor = #cc5777
mainColorSub = #f3abbf
secondColor = #45a9bc
secondColorSub = #98ccd6
grayBgColor = #f7f7f7
grayTxtColor = #333333
grayTagColor = #828282
grayLineColor = #d1d1d1
redColor = #bf0000


orientalBlueColor = #002896
poppyRedColor = #ff41be
signalredColor = #d30098
HyacinthColor = #edeeff
LavenderBlushColor = #FFECF8
CreamBruleeColor = #FFE99B

whiteColor = #fff
blackColor = #000

pcWidth = 1216px

//------------------------------

#Rakuma // #Pagenameはページ固有の名前に置換してください

  // 共通パーツは_common/stylus/Module/Common/_campaignParts.stylにあります
  // 各ページでカスタム使用したい場合はclass名で上書きしてください

  // common
  .textLink
    color: secondColor
    &:hover
      opacity: .8

  navHeightPc = 82px
  navHeightSp = 70px

  #EntryBox,
  #StepRakuma,
  #IntroductionRakuma,
  #Detail
    display: block
    +pc()
      padding-top: (navHeightPc + 0)
      margin-top: -(navHeightPc + 0)
    +sp()
      padding-top: (navHeightSp + 0)
      margin-top: -(navHeightSp + 0)


  // override
  .titleWrap
    +pc()
      margin-bottom: 40px
    +sp()
      margin-bottom: 24px

  // main
  .main
    margin-bottom: 40px
    +sp()
      margin-bottom: 24px
    .main__title
      display: block
      width:pcWidth
      margin: 0 auto
      line-height:1
      font-size:1.4rem
      text-align: center
      +sp()
        width:100%
        margin: 0 auto
      &Img
        width:100%
    .main__period
      width: 100%
      padding: 10px
      background: orientalBlueColor
      box-sizing: border-box
      +pc()
        width: pcWidth
        margin: 0 auto 27px
      +sp()
        width: 100%
        margin: 0 auto 14px
      .main__period__text
        color: white
        text-align: center
        font-weight: bold
        line-height: 1.5
        +pc()
          font-size: 1.8rem
        +sp()
          font-size: 1.2rem
        .main__period__text__terms
          +pc()
            display: inline-block
            padding-left: 24px
          +sp()
            display: block
    .main__caption
      +pc()
        width: 1000px
        margin: 0 auto 36.5px
      +sp()
        width: calc(100% - 32px)
        margin: 0 auto 28.5px
    .main__terms
      +pc()
        width: 968px
        margin: 0 auto 36.5px
      +sp()
        width: calc(100% - 32px)
        margin: 0 auto 34px
      .main__terms--text
        line-height: 1.4
        +pc()
          display: inline
          font-size: 1.6rem
        +sp()
          display: block
          font-size: 1.1rem
        .main__terms--text-bold
          font-weight: bold
          line-height: 1.5

  // navigation
  .nav
    position: sticky
    z-index: 10
    top: 0
    width: 100%
    background: rgba(256,256,256,0.8)
    +pc()
      margin-bottom: 30px
      padding: 6px 0
    +sp()
      margin-bottom: 21.5px
    .nav__list__wrap
      display: flex
      justify-content: center
      align-items: center
      height: 70px
      box-sizing: border-box
      +pc()
        width: 890px
        margin: 0 auto
      +sp()
        width: 100%
        padding: 0 16px
      .nav__list
        width: 25%
        height: 58px
        +pc()
          width: 271px
          margin-right: 39px
        +sp()
          width: 31.4%
          margin-right: 9.5px
        &:last-of-type
          margin-right: 0
        .nav__list--btn
          display: flex
          justify-content: center
          align-items: center
          position: relative
          width: 100%
          height: 100%
          background: poppyRedColor
          border: 2px solid signalredColor
          box-shadow: 0px 3px 0px 0px rgba(211, 0, 152, 0.40)
          border-radius: 10px
          box-sizing: border-box
          text-align: center
          +pc()
            padding-bottom: 16px
          +sp()
            padding-bottom: 10px
          &::after
            position: absolute
            bottom: 9px
            width: 0
            height: 0
            border-left: 9px solid transparent
            border-right: 9px solid transparent
            border-top: 10px solid white
            content: ""
            +sp()
              border-width: 8px 6px 0 6px;
              border-color: #FFFFFF transparent transparent transparent;
              bottom: 3px
          .nav__list--btn-text
            display: inline-block
            position: relative
            color: white
            font-weight: bold
            +pc()
              font-size: 1.6rem
              line-height: 1.4
            +sp()
              font-size: 1.1rem
              line-height: 1.4

  // Step Header common
  .step--title__wrap
    width: 100%
    box-sizing: border-box
    text-align: center
    line-height: 1.4
    &.-pink
      background: poppyRedColor
    &.-blue
      background: orientalBlueColor
      .step--title
        +sp()
          font-size: 2.4rem
    .step--title
      position: relative
      color: white
      font-weight: bold
      padding: 16px 0
      +pc()
        margin: 0 auto
        font-size: 3.8rem
      +sp()
        font-size: 3.2rem
      span.-yellow
        color: CreamBruleeColor
        +pc()
          font-size: 4.2rem
        +sp()
          font-size: 2.8rem 
      .step--title--caption
        font-weight: normal
        width: 100%
        margin-top: 5px
        +pc()
          font-size: 1.4rem
        +sp()
          font-size: 1rem
  .step__title__img
    display: flex
    justify-content: center
    align-items: center
    margin: 0 auto 30px
    background: white
    border-radius: 12px
    box-sizing: border-box
    +pc()
      width: 342px
      height: 80px
      padding: 19px 30px
    +sp()
      width: 278px
      height: 69px
      padding: 19px 30px

  // entry btn
  .entryBox
    +pc()
      margin-bottom: 60px
    +sp()
      margin-bottom: 40px
    .entryBox__container
      margin: 0 auto
      +pc()
        width: 100%
        margin: 40px auto
      +sp()
        width: calc(100% - 32px)
        margin-top: 24px
      .entryBox__container--title
        text-align: center
        font-weight: bold
        line-height: 1.4
        +pc()
          margin: 40px auto
          font-size: 3.2rem
        +sp()
          margin-bottom: 24px
          font-size: 2.2rem


    // ReX Entry btn
    @import passStr + '_common/stylus/Module/Common/_btnEntry'
    btnEntry()

    // paermission text
    @import passStr + '_common/stylus/Module/Common/_mailPermissionTxt'
    mailPermissionTxt()

    // 終了対応
    // ボタンセット(bgColor,txtColor)
    btnSet(grayTagColor, whiteColor)


  // step2
  .stepRakuma
    .stepRakuma_wrapper
      width: 100%
      background: HyacinthColor
      +pc()
        padding: 40px 0 60px
      +sp()
        padding: 24px 0 50px
      .stepRakuma__flow
        +sp()
          width: calc(100% - 32px)
          margin: 0 auto
        .step__flow
          display: flex
          margin: 0 auto
          +pc()
            width: 582px
          +sp()
            flex-direction: column
          .step__flow--lead
            font-weight: bold
            line-height: 1.4
            +pc()
              width: 355px
              margin-right: 32px
              font-size: 3.2rem
            +sp()
              font-size: 2.2rem
              text-align: center
              margin-bottom: 30px
            .-blue
              color: orientalBlueColor
          .step__flow__img
            +pc()
              width: 195px
            +sp()
              width: 239px
              margin: 0 auto 30px
          .step__flow__info
            font-weight: normal
            line-height: 1.4
            +pc()
              font-size: 1.4rem
              padding-top: 20px
            +sp()
              padding: 0 16px
              font-size: 1.2rem
              text-align: center
        .step__rakuma_app
          background: whiteColor
          border-radius: 10px
          max-width: 818px
          margin: 0 auto
          text-align: center
          box-sizing: border-box
          +pc()
            padding: 40px
          +sp()
            padding: 24px
          .step__rakuma_app__txt
            font-weight: bold
            line-height: 1.4
            +pc()
              font-size: 2.4rem
              margin-bottom: 32px
            +sp()
              font-size: 1.8rem
              margin-bottom: 16px
          .step__rakuma_app__link
            display: flex
            justify-content: center
            .step__rakuma_app__icon
              +pc()
                height: 85px
                margin-right: 24px
              +sp()
                height: 60px
                margin-right: 16px
              img
                height: 100%
                width: auto
            .step__rakuma_app__dl
              +pc()
                height: 85px
                display: flex
              .step__rakuma_app__dl__btn
                +pc()
                  margin-right: 24px
                  &:first-child
                    width: 286px
                  &:last-child
                    width: 231px
                +sp()
                  height: 60px
                img
                    height: 100%
              &.-app
                .step__rakuma_app__dl__btn
                  width 200px
                  height: auto
                  margin-bottom: 20px
                  img
                    width 100%
                    height: auto

  // Step3&4
  .stepBeauty
    padding-bottom: 2px
    overflow: hidden
    .stepBeauty_wrapper
      background: LavenderBlushColor
      +pc()
        padding: 40px 0
      +sp()
        padding: 24px 0
      .step__flow
        font-weight: bold
        line-height: 1.4
        text-align: center
        +pc()
          margin: 40px auto
          font-size: 3.2rem
        +sp()
          font-size: 1.8rem
          text-align: center
          margin-bottom: 30px
        .-pink
          color: poppyRedColor
      .conditionsBeauty__menu
        +pc()
          display: flex
          width: 950px
          margin: auto
          padding-bottom: 32px
        +sp()
          margin-left calc((100% - 280px) / 2) !important
          padding-top: 22px !important
          &:last-of-type
            margin-left: 0
            margin-right calc((100% - 280px) / 2) !important
        .conditionsBeauty__menu--list
          position: relative
          margin-right: 36px
          padding: 26px 20px 0
          background: whiteColor
          border-radius: 14px
          box-sizing: border-box
          text-align: center
          +pc()
            width: 294px
          +sp()
            width: 280px !important
          &:nth-of-type(-n+2)::after
            +pc()
              position: absolute
              top: 50%
              right: -26px
              width: 0
              height: 0
              border-top: 13px solid transparent
              border-left: 13px solid grayTagColor
              border-bottom: 13px solid transparent
              transform: translateY(-50%)
              content: ''
          &:last-of-type
            +pc()
              margin-right: 0
              padding: 26px 20px 7px
            +sp()
              padding: 32px 20px 14px
            .conditionsBeauty__menu--list__img
              height: 160px
          .conditionsBeauty__menu--list--icon
            position: absolute
            left: 50%
            transform: translate(-50%,0)
            top: -21px
            width: 43px
            height: 43px
            background: poppyRedColor
            border-radius: 50%
            color: whiteColor
            text-align: center
            font-size: 1.8rem
            font-weight: bold
            line-height: 2.3
          .conditionsBeauty__menu--list--lead
            padding-bottom: 12px
            font-weight: bold
            line-height: 1.6
            text-align: center
            +pc()
              font-size: 1.8rem
            +sp()
              font-size: 1.4rem
          .conditionsBeauty__menu--list__img
            +pc()
              height: 144px
            +sp()
              height: 158px

  //- splide スライダー
  .splide
    +sp()
      width: 280px
      margin: 0 auto 24px
  .splide__track
    +sp()
      overflow: visible !important
  .splide__arrow.splide__arrow--next:disabled
    display: none !important
  .splide__pagination
    bottom: -26px
  .splide__arrow:disabled
    display: none !important
  .splide__pagination__page
    width: 6px
    height: 6px
    background #ccc
  .splide__pagination__page.is-active
    width: 6px
    height: 6px
    background grayTagColor
    transform: scale(1)
  .splide__arrow
    display: block
    position: absolute !important
    top: 50% !important
    width: 0 !important
    height: 0 !important
    background: transparent !important
    border-radius: 0 !important
    transform: translateY(-50%) !important
    content: '' !important
    &.splide__arrow--next
      right: -25px !important
      border-top: 15px solid transparent !important
      border-left: 15px solid grayTagColor !important
      border-bottom: 15px solid transparent !important
    &.splide__arrow--prev
      left: -25px !important
      border-top: 15px solid transparent !important
      border-right: 15px solid grayTagColor !important
      border-bottom: 15px solid transparent !important
    svg
      display: none

  //- 共通ボタン
  .conditionsBox__search__wrapper
    +sp()
      width: calc(100% - 32px)
      margin: 0 auto
  .conditionsBox__search
    margin: 0 auto 30px
    +pc()
      width: 600px
      margin-top: 32px
    +sp()
      padding-top: 24px
      margin-bottom: 15px
    .conditionsBox__search--top
      text-align: center
      margin-bottom: 10px
      +pc()
        font-size: 1.6rem
      +sp()
        font-size: 1.4rem        
    .conditionsBox__search--button
      display: flex
      justify-content: center
      align-items: center
      margin: auto
      border-radius: 44px
      color: whiteColor
      text-align: center
      line-height: 1.5
      &:hover
        opacity 0.8
      +pc()
        font-size: 3.6rem
        height: 88px
      +sp()
        box-sizing: border-box
        font-size: 2.2rem
        height: 74px
      &.-blue
        background: orientalBlueColor
      &.-pink
        background: poppyRedColor
    .conditionsBox__search--button--info
      padding-top: 8px
      text-align: center
      line-height: 1.8
      +pc()
        font-size: 1.6rem
      +sp()
        font-size: 1.4rem
      a
        color: secondColor
    .conditionsBox__search--button--annotation
      padding-top: 8px
      text-align: center
      line-height: 1.8
      +pc()
        font-size: 1.4rem
      +sp()
        font-size: 1.2rem

  //- ポイントGETイメージ
  .stepLast
    text-align: center
    position: relative
    margin-bottom: 30px
    +pc()
      padding-top: 60px
    &:before
      position: absolute;
      top: -16px;
      right: 0
      left: 0
      margin: auto
      width: 0;
      height: 0;
      content: "";
      +pc()
        border-right: 36px solid transparent;
        border-left: 36px solid transparent;
        border-top: 30px solid #0a32e2;
      +sp()
        border-right: 25px solid transparent;
        border-left: 25px solid transparent;
        border-top: 25px solid #0a32e2;
    .step__last__img
      margin-bottom: 30px
    .step__last__notice
      line-height: 1.5
      text-align: left
      max-width: 820px
      margin: 0 auto
      +pc()
        font-size: 1.4rem
      +sp()
        font-size: 1.2rem
        width: calc(100% - 32px)


  //- アプリDL
  .beautyintro__container
    position: relative
    width: 100%
    background: LavenderBlushColor
    box-sizing: border-box
    text-align: center
    +pc()
      margin-bottom: 48px
      padding: 50px 16px 40px
    +sp()
      margin-bottom: 56px
      padding: 32px 16px 24px
    &:last-of-type
      margin-bottom: 0
    .beautyintro__lead__wrap
      display: flex
      justify-content: center
      align-items: center
      .beautyintro__lead__icon__point
        +pc()
          margin-right: 14px
          width: 52px
        +sp()
          margin-right: 12px
          width: 49px
      .beautyintro__lead__icon__deal
        width: 95px
        margin-right: 14px
      .beautyintro__lead__icon__appicon
        width: 45px
        margin-right: 12px
        +pc()
          width: 55px
      .beautyintro__lead--lead
        font-weight: bold
        text-align: center
        line-height: 1.4
        +pc()
          font-size: 2.4rem
        +sp()
          font-size: 1.8rem
    .beautyintro__lead--lead--info
      font-size: 1.2rem
      line-height: 1.4
      text-align: center
      +pc()
        padding-top: 21.5px
      +sp()
        padding-top: 16px
    .beautyintro__menu
      display: flex
      align-items: center
      justify-content: center
      +pc()
        margin-top: 32px
      +sp()
        flex-wrap: wrap
        margin-top: 15.5px
      .beautyintro__menu__list
        +pc()
          width: 110px
          margin-right: 16px
          &:last-of-type
            margin-right: 0
        +sp()
          width: 92px
          margin-right: 7.5px
          &:nth-child(-n+3)
            margin-bottom: 7.5px
          &:nth-child(3n),
          &:last-of-type
            margin-right: 0
    .beautyintro__menu--info
      text-align: center
      line-height: 1.6
      +pc()
        padding-top: 32px
        font-size: 1.8rem
      +sp()
        padding-top: 16px
        font-size: 1.4rem
    .beautyintro__menu__img
      +pc()
        width: 600px
        margin: 32px auto 0
      +sp()
        width: calc(100% - 87px)
        margin: 16px auto 0
    .beautyintro__menu__button__wrap
      +pc()
        width: 600px
        margin: 32px auto 0
      +sp()
        width: calc(100% - 32px)
        margin: 16px auto 0
      .beautyintro__menu__button
        display: inline-block
        width: 100%
        padding: 16px 24px
        background: CherryPinkColor
        border-radius: 44px
        box-sizing: border-box
        color: whiteColor
        text-align: center
        font-weight: bold
        line-height: 1.5
        +pc()
          font-size: 3rem
        +sp()
          font-size: 1.6rem
    .beautyintro__menu__app__wrap
      +pc()
        width: 632px
        margin: 32px auto 0
      +sp()
        width: 100%
        margin: 16px auto 0
      .beautyintro__menu__app__banner
        display: inline-block
        width: 100%
    .beautyintro__menu__app__link__wrap
      +pc()
        width: 632px
        margin: 32px auto 0
      +sp()
        width: 61%
        margin: 16px auto 0
      .beautyintro__menu__app__article
        +pc()
          display: flex
          align-items: center
          justify-content: center
          width: 530px
          margin: auto
        .beautyintro__menu__app__img
          +pc()
            width: 268px
            margin-right: 12px
        .beautyintro__menu__app__qr
          +pc()
            width: 250px
            text-align: center
          .beautyintro__menu__app__qr--text
            +pc()
              display: inline-block
              position: relative
              margin-top: 24px
              font-size: 2rem
              font-weight: bold
              line-height: 1.4
              &::before
                content: "|"
                position: absolute
                top: 0
                left: -1.5rem
                font-weight: normal
                transform: rotate(-45deg)
              &::after
                content: "|"
                position: absolute
                top: 0
                right: -1.5rem
                font-weight: normal
                transform: rotate(45deg)
          .beautyintro__menu__app__qr__img
            +pc()
              display: block
              width: 192px
              margin: 24px auto 0
      .beautyintro__menu__app__button
        +sp()
          display: block
          width: 100%

  // ラクマとは
  .introductionRakuma
    .introductionRakuma__intro
      +pc()
        display: flex
        flex-direction: row-reverse
        align-items: center
        justify-content: center
        font-size: 2.4rem
        margin:40px auto 48px
        line-height: 1.8
      +sp()
        font-size: 1.4rem
        line-height: 1.4
        text-align: center
        margin: 24px auto 30px
      .introductionRakuma__intro__txt
        +sp()
          margin-bottom: 30px
      .introductionRakuma__intro__img
        max-width: 360px
        +pc()
          margin-right: 30px
        +sp()
          width: calc(100% - 32px)
          margin: 0 auto
    .introductionRakuma__merit
      +pc()
        width: 850px
        margin: 0 auto 60px
      +sp()
        width: calc(100% - 32px)
        margin: 0 auto 48px
      .introductionRakuma__merit__title
        font-weight: bold
        text-align: center
        line-height: 1.4
        +pc()
          font-size: 3.2rem 
          margin-bottom: 30px
        +sp()
          font-size: 2.2rem 
          margin-bottom: 16px
      .introductionRakuma__merit__list 
        display: flex
        text-align: center
        justify-content: space-between
        margin: 0 auto 30px  
        +sp() 
          margin-bottom: 16px
          max-width: 380px
        .introductionRakuma__merit__list__img
          +pc() 
            margin-bottom: 24px
          +sp() 
            margin-bottom: 10px
          img
            +pc() 
              max-height: 194px
            +sp() 
              max-height: 77px
        .introductionRakuma__merit__list__txt
          font-weight: bold
          line-height: 1.4
          +pc() 
            font-size: 2.4rem
          +sp() 
            font-size: 1.2rem
          .introductionRakuma__merit__list__txt__cap
            font-size: 0.5em;
            vertical-align: top;
            font-weight: normal
      .introductionRakuma__merit__annotation 
        line-height: 1.4
        +pc()
          font-size: 1.4rem
        +sp()
          font-size: 1.2rem

  // バーコード出品
  .barcodeRakuma
    background: HyacinthColor
    +pc()
      padding: 0 0 40px
      margin-bottom: 40px
    +sp()
      padding: 0 0 15px
      margin-bottom: 24px
    .barcodeRakuma__box
      background: whiteColor
      max-width: 818px
      margin: 0 auto 30px
      text-align: center
      box-sizing: border-box 
      +pc()
        padding: 30px
        margin-top: 30px
      +sp()
        width : calc(100% - 32px)
        padding: 15px
        margin-top:24px
      .barcodeRakuma__point
        margin: 0 auto 30px
        text-align: center
        .barcodeRakuma__point__barcode__img 
          max-width: 584px
          +pc()
            margin: 0 auto 30px
          +sp()
            margin: 0 auto 20px
        .barcodeRakuma__point__list
          display: flex
          margin: 0 auto
          justify-content: center
          .barcodeRakuma__point__list__point
            +pc()
              width 290px
              box-sizing: border-box
            +sp()
              width 50%
              box-sizing: border-box
          .barcodeRakuma__point__list__point:first-child
            +pc()
              border-right: 4px solid #EBEBEB
              padding-right: 40px
            +sp()
              border-right: 1px solid #EBEBEB
              padding-right: 10px
          .barcodeRakuma__point__list__point:last-child
            +pc()
              padding-left: 40px
            +sp()
              padding-left: 10px
          .barcodeRakuma__point__list__point__head
            color: orientalBlueColor
            font-weight: bold
            +pc()
              font-size: 2.0rem
              margin-bottom: 16px
            +sp()
              font-size: 1.4rem
              margin-bottom: 12px
