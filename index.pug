extends ../../../../_common/pug/Base/_layout.pug

block init
  -
    var parameter = "?v20250119"
    var meta = {
      title: "SPU(スーパーポイントアッププログラム）",
      directory: "spu-figma",
      description: "SPU（スーパーポイントアッププログラム）は、楽天グループサービスを使えば使うほどポイントアップするお得なプログラムです。通常ポイント1倍に加えて、楽天ビューティで当月3,000円(税込)以上のメニューでネット予約して施術完了すると、その月の楽天市場のお買い物が更にポイント+0.5倍に！",
      url: "https://beauty.rakuten.co.jp/cnt/topics/campaign/spu-figma/",
      og_image: "https://beauty.rakuten.co.jp/cnt/topics/campaign/spu/img/ogp_spu.png" + parameter,
    }
    var css = "../spu/css/main.css" + parameter

block addHead
  //- nofollowが必要なときはコメントを外す
  //- meta(name='robots', content='noindex,nofollow')
  //- PITARI_area_cpnlist（PITARI関連jsは読み込み順調整のためdeferをつけない / WebBlocksを使わない時のみ使用）
  script(src='//www.rakuten.co.jp/com/advance/beauty/AreaTarget_SPU_2021/responsive/condition.js')
  link(rel='stylesheet', href="../spu/css/splide.min.css")

block nav
  //- grobalNav
  +grobalNav('/cnt/topics/')

  //- breadcrumb
  -
    var lists = [
      {
        name: 'キャンペーン一覧・特集',
        link: 'https://beauty.rakuten.co.jp/cnt/topics/',
      },
      {
        name: '【SPU】楽天ビューティ月1回3,000円（税込）以上のネット予約＆施術完了でポイント＋0.5倍',
        link: 'current', //- 'current' is no link
      },
    ]
  +breadcrumb(lists)

block content

  #Spu.spu

    #CpnTags
      -
        var taglist = {
          tag: [
            'エントリー不要', //- エントリー不要は自動的に青色になります
            'ネット予約・施術完了',
            '常時開催',
