passStr = '../../../../../'
@import passStr + '_common/stylus/Base/_init'

//------------------------------// Common - このページで使う共通設定。自由に設定可能

secondColor = #45a9bc
grayBgColor = #f7f7f7
grayTxtColor = #0E3A3B
grayTagColor = #828282
grayLineColor = #d1d1d1

followBtnColor = #FF3939
stepColor = #0C0278
backmainColor = #FFD7D7
backsecColor = #FF8888
underlineColor = #FCEE21
underlineColor_sec = #EB7605
highlighttext = #CA080B

whiteColor = #fff
blackColor = #000
greenColor = #27BBC7
blueColor = #2D8BBE
orangeColor = #FF6060
babyblueColor = #BFE7EF
secondbabyblueColor = #A5DDE9
babypinkColor = #FFEBEB
rbpinkColor = #DF205F

pcWidth = 1216px

//------------------------------

#X // #Pagenameはページ固有の名前に置換してください

  // 共通パーツは_common/stylus/Module/Common/_campaignParts.stylにあります
  // 各ページでカスタム使用したい場合はclass名で上書きしてください
  // common
  // override
  .titleWrap
    +pc()
      margin-bottom: 24px
    +sp()
      margin-bottom: 16px
  .titleWrap--info
    line-height: 1.8
    +pc()
      width: 100%
      margin-bottom: 24px
      font-size: 1.4rem
    +sp()
      width: calc(100% - 20px)
      margin: 0 auto 16px
      font-size: 1.2rem
  .txtLink
    color:secondColor
    text-decoration: underline
    &:hover
      text-decoration: none
  a
    &:hover
      opacity: .8 !important
      transition-duration: 0.15s !important

  navHeightPc = 80px
  navHeightSp = 58px

  #Prize,
  #StepAll,
  #StepNew,
  #Anniversary_section,
  #Detail
    display: block
    +pc()
      padding-top: (navHeightPc + 0)
      margin-top: -(navHeightPc + 0)
  #StepAll,
  #StepNew,
  #Anniversary_section,
  #Detail
    +sp()
      padding-top: (navHeightSp + 0)
      margin-top: -(navHeightSp + 0)
  // main
  .main
    margin-bottom: 60px
    +sp()
      margin-bottom:0
    .main__title
      display: block
      width:pcWidth
      margin: 0 auto
      line-height:1
      font-size:1.4rem
      text-align: center
      +sp()
        width:100%
        margin: 0 auto
      &Img
        width:100%
    .main__terms
      text-align: center
      padding: 16px 10px
      margin: 0 auto 
      box-sizing: border-box
      background: grayBgColor
      +pc()
        width:pcWidth
      .main__termsBox
        width: 100% //IE対応
        margin: 0 auto
        .main__termsTxt
          line-height 1.5
          color:grayTxtColor
          font-weight: bold
          +pc()
            display: block
            font-size: 1.4rem
          +sp()
            font-size: 1.4rem
          span.main__termsTxt-small
            display: inline-block
            color:grayTxtColor
          &.-bold
            font-weight: bold
    .main__terms__summary__wrap
      margin: 0 auto
      +pc()
        width: pcWidth
      +sp()
        width: 100%
        margin: 0 auto
      .main__terms__summary
        display: flex
        justify-content: center
        align-items: center
        box-sizing: border-box
        +pc()
          // padding: 16px 60px
          border-radius: 16px
          width: 1004px
          margin: 0 auto
        +sp()
          flex-direction: column
          padding: 16px
        .main__terms__summary__step
          +pc()
            width: 593px
            margin-right: 36px
          +sp()
            width: 100%
          .main__terms__summary__all
            display: block
            +sp()
              margin-bottom: 16px
          .main__terms__summary__new
            display: block
            +sp()
              margin-bottom: 8px
        .main__terms__summary__point
          +pc()
            width: 331px
          +sp()
            width: 100%
      .main__terms__summary--info
        +pc()
          font-size: 1.2rem
          line-height: 1.5
          width:818px
          margin: 0 auto 
          padding-top: 16px
        +sp()
          width: calc(100% - 32px)
          margin: auto
          font-size: 1rem
          line-height: 1.4
        .main__terms__summary--info-bold
          font-weight: bold

//- anchor link
  .globalMenu
    position: sticky
    top: 0
    z-index: 1000
    background: navyColor
    border-bottom: 1px solid grayLineColor
    .globalMenu-inner
      display: flex
      width: 100%
      margin: 0 auto
      .globalMenu-list
        position: relative
        width: 25%
        box-sizing: border-box
        color: whiteColor
        font-size: 1.1rem
        font-weight: bold
        +sp()
          font-size: 1.2rem
          height: 38px
        &:not(:last-child)::before
          display: inline-block
          position: absolute
          height: 1px
          transform: rotate(90deg)
          background-color: grayLineColor
          border-right: 1px solid grayLineColor
          content: ''
          +pc()
            top: 26px
            right: -16px
            width: 32px
          +sp()
            top: 31px
            right: -19px
            width: 38px
        .globalMenu-button
          display: flex
          justify-content: center
          position: relative
          width: 100%
          text-align: center
          line-height: 1.5
          +pc()
            align-items: center
            height: 60px
            padding: 10px 0
          +sp()
            align-items: flex-start
            height: 38px
            padding: 10px 0
          &.-profit
            background: greenColor
          &.-stepall
            background: followBtnColor
          &.-stepnew
            background: blueColor
          &.-anniv
            background: orangeColor
          &::before
            content: ""
            position: absolute
            left: 50%
            width: 8px
            height: 8px
            border-top: solid 1px white
            border-right: solid 1px white
            transform: translateX(-50%) rotate(135deg)
            +pc()
              bottom: 12px
            +sp()
              bottom: 9px
          &:hover
            cursor: pointer
            color: white
            .globalMenu-text
              opacity: 0.8
              transition-duration: 0.15s
          &:hover::before
            border-top: solid 1px
            border-right: solid 1px
            opacity: 0.8
            transition-duration: 0.15s



          .globalMenu-text
            display: block
            flex-grow: 0
            color: white
            font-size: 1.1rem
            line-height: 1.2
            +pc()
              padding: 0 0 12px
              font-size: 1.6rem
              line-height: 1.3
            +sp()
              margin: 0 2px
              font-size: 1.2rem
              line-height: 1.1

//- Prize
  .prize
    background-color: babyblueColor
    +sp()
      padding-top: 38px!important
      margin-top:-38px
    .prizeContent__wrap
      width: 750px
      padding: 60px 35px
      margin: 0 auto
      +sp()
        width: 340px
        padding: 40px 16px
      .prizeContent__termsBox
        width: 100% //IE対応
        margin: 0 auto
        .prizeContent__termsTxt
          text-align: center
          line-height: 1.8
          +pc()
            margin-top: 20px
            font-size: 1.4rem
          +sp()
            width: calc(100% - 32px)
            margin: 10px auto 0
            font-size: 1.2rem

        .prizeContent__termsLink
          color:secondColor
          align-items: stretch
          justify-content: center
          +pc()
            width: 780px
            margin: 10px auto
          +sp()
            width: 100%
            max-width: 800px
            margin: 16px auto
          &:hover	
            opacity: .8	
            transition-duration: 0.15s
            // text-decoration: none



  // フォローボタン
  .buttonSection
    display: flex
    align-items: center
    width: 100%
    background: whiteColor
    +sp()
      height: 75px
    @media screen and (min-width: 1247px) {
      width: 100%
      height: 120px
    }
    @media screen  and (min-width: 941px) and (max-width: 1246px) {
      width: 1246px
      height: 120px
    }
    &.-fixed
      position: fixed
      bottom: 0
      left: 0
      background: rgba(255, 255, 255, 0.5)
      backdrop-filter: blur(2px)
      text-align: center
      +pc()
        transform: translateY(120px)
      +sp()
        transform: translateY(80px)
    &.-remove-item
      z-index: -1
    &.-active
      +pc()
        right: 0
        animation: slide_active_pc 0.4s forwards
        z-index: 13
      +sp()
        animation: slide_active_sp 0.4s forwards
        z-index: 13
    &.-remove
      +pc()
        animation: slide_remove_pc 0.4s forwards
        z-index: 13
      +sp()
        animation: slide_remove_sp 0.4s forwards
        z-index: 13
    &.-remove-item.-top-remove
      +pc()
        animation: slide_remove_pc 0.4s forwards
        z-index: 13
      +sp()
        animation: slide_remove_sp 0.4s forwards
        z-index: 13
    .buttonSection__button--link
      display: inline-block
      margin: auto
      background: followBtnColor
      border-radius: 44px
      color: whiteColor
      +pc()
        width: 600px
        padding: 20px 0
        font-size: 3.6rem
        line-height: 1.3
      +sp()
        width: calc(100% - 61px)
        padding: 16px 0
        font-size: 1.8rem
        line-height: 1.5
      &:hover
        opacity: .8
        transition-duration: 0.15s
      // //終了用ボタン
      // &.endButton
      //   background-color: grayTagColor
      //   color: white
      //   pointer-events: none

    @keyframes slide_active_pc {
      0% {
        transform: translateY(120px)
      }
      100% {
        transform: translateY(0)
      }
    }

    @keyframes slide_active_sp {
      0% {
        transform: translateY(80px)
      }
      100% {
        transform: translateY(0)
      }
    }

    @keyframes slide_remove_pc {
      0% {
        transform: translateY(0)
      }
      100% {
        transform: translateY(120px)
      }
    }

    @keyframes slide_remove_sp {
      0% {
        transform: translateY(0)
      }
      100% {
        transform: translateY(80px)
      }
    }


  // entry btn
  .entryBox
    +pc()
      margin-bottom: 40px
    +sp()
      margin: 0 16px 24px

    // エントリーボタン幅設定 #rex-ceb-01以外のボタンに反映される。importantをつければ全てに反映される
    .CampaignButton .RexButton
      max-width: 600px
      padding: 16px 24px
    // エントリーボタンチェックボックス下とテキストの間の余白
    #rex-ceb-01,
    #rex-ceb-02,
    #rex-ceb-03
      margin-bottom: 16px
    //　エントリーボタン 通常
    .CampaignEntryButton_campaignButton__n6hK5 .CampaignEntryButton_rexButton__VH0Gh.CampaignEntryButton_red__2Uuem
      background-color: blueColor
      border-color: blueColor
      &:hover
        opacity: .8 !important
        background-color: blueColor!important
        border-color: blueColor!important

    //　エントリーボタン エントリー後
    .CampaignEntryButton_campaignButton__n6hK5 .CampaignEntryButton_rexButton__VH0Gh.CampaignEntryButton_red__2Uuem.disabled
      background-color: #dfdfdf
      border-color: #dfdfdf
    //　エントリーボタン ローディング
    .CampaignEntryButton_campaignButton__n6hK5 .CampaignEntryButton_loading__lYu0i.CampaignEntryButton_rexButton__VH0Gh.CampaignEntryButton_red__2Uuem.disabled
      background-color: blueColor
      border-color: blueColor
    //　エントリーボタン エントリー完了
    .CampaignEntryButton_success__GLKNB
      background: #ebf7ec !important
      border-color: #ebf7ec !important
    //　エントリーボタン チェックボックスチェックあり
    .Permission_checkbox__2N5q8:checked+.Permission_icon__uXZyD.Permission_red__wstq2
      background-color: blueColor
      border-color: blueColor
    //　エントリーボタン 上部スペース
    .CampaignEntryButton_campaignButton__n6hK5
      +sp()
        margin: 12px auto
    // ReX Entry btn
    @import passStr + '_common/stylus/Module/Common/_btnEntry'
    btnEntry()

    // paermission text
    @import passStr + '_common/stylus/Module/Common/_mailPermissionTxt'
    mailPermissionTxt()

    // 終了対応
    // ボタンセット(bgColor,txtColor)
    btnSet(grayTagColor, #fff)


  // step
  .stepContent
    position: relative
    width: 100%
    text-align: center
    +pc()
      margin: auto
      padding: 100px 0 60px
    +sp()
      padding: 80px 0 40px
    &::after
      position: absolute
      z-index: 5
      bottom: -40px
      left: 0
      right: 0
      width: 0
      height: 0
      margin: 0 auto
      border-style: solid
      border-color: whiteColor transparent transparent transparent
      content: ""
      +sp()
        border-width: 40px 49vw 0 49vw
        overflow: hidden
      @media screen and (min-width: 1247px) {
        border-width: 40px 49vw 0 49vw
        overflow: hidden
      }
      @media screen  and (min-width: 941px) and (max-width: 1246px) {
        border-width: 40px 49vw 0 49vw
        overflow: hidden
      }
    &.-first
      +pc()
        padding:0 0 60px
      +sp()
        padding: 0 0 40px
    &.-step2
      background:  backmainColor
      &.-new
        background:  babyblueColor
        &::after
          border-color: babyblueColor transparent transparent transparent
      &::after
        border-color: backmainColor transparent transparent transparent
    &.-step3
      background: backsecColor
      color: grayTxtColor
      &.-new
        background: secondbabyblueColor
        &::after
          border-color: secondbabyblueColor transparent transparent transparent
    &.-stepresult
      padding: 80px 16px 40px
      +sp()
        padding :auto 16px
      &.-stepall
        background: #FF8888
        color: #0E3A3B
      &.-new
        background: blueColor
        color: whiteColor
      &::after
        content: none
      +sp()
        padding :auto 16px
      +pc()
        margin: auto
        padding: 100px 0 60px
      +sp()
        padding: 80px 0 40px
      
    .stepContent__terms
      text-align: center
      padding: 16px 10px
      margin: 40px auto 60px
      box-sizing: border-box
      background: grayBgColor
      +pc()
        width: 786px
      +sp()
        max-width: 800px
        margin: 16px 16px 40px
      .stepContent__terms--list
        font-size: 1.6rem
        line-height 1.5
        color:grayTxtColor
        +sp()
          font-size: 1.4rem
        .stepContent__terms--list-bold
          font-weight: bold
    .stepContent--leadtitle__wrap
      width: 100%
      box-sizing: border-box
      text-align: center
      line-height: 1.4
      &.-orange
        background: followBtnColor
      &.-blue
        background: blueColor
      .stepContent--leadtitle
        display: inline-block
        position: relative
        color: white
        font-weight: 700
        padding: 16px 0
        +pc()
          margin: 0 auto
          font-size: 3.6rem
        +sp()
          font-size: 2.4rem
        span.stepContent--leadtitle-big
          font-size: 4.8rem	
          letter-spacing: 1px
          +sp()	
              font-size: 3.8rem
        span.stepContent--leadtitle-small
          font-size: 3.2rem
          +sp()
            font-size: 2rem
          &::before
            content: "|"
            position: absolute
            top: 0
            font-weight: normal
            transform: rotate(-45deg)
            +pc()
              top: 2.5rem
              left: 5.5rem
              font-size: 2.8rem
            +sp()
              top: 2rem
              left: 4rem
              font-size: 2rem
          &.-new
            &::before
              +pc()
                left: 8rem
              +sp()	
                left: 6rem
          &::after
            content: "|"
            position: absolute
            top: 0
            font-weight: normal
            transform: rotate(45deg)
            +pc()
              top: 2.5rem
              right: 5.5rem
              font-size: 2.8rem
            +sp()
              top: 2rem
              right: 4rem
              font-size: 2rem
          &.-new
            &::after
              +pc()
                right: 8rem
              +sp()	
                right: 6rem



    .stepContent--title
      margin: auto
      +pc()
        width: 200px
      +sp()
        width: 150px
    .stepContent--text--wrap
      +pc()
        font-size: 1.4rem
        line-height: 1.5
        margin:35px auto
      +sp()
        font-size: 1.2rem
        line-height: 1.4
        margin:40px auto 24px
      .stepContent--text
        font-weight: bold
        font-size: 2rem
        line-height: 1.4
      .stepContent--text--info
        margin-top: 8px
        font-size: 1.4rem
        +pc()
          line-height: 1.5
        +sp()
          line-height: 1.4

    .stepContent__pandaicon__container
      .stepContent__pandaicon__menu
        .stepContent__pandaicon__menu__list
          .stepContent__pandaicon__menu__list--text__wrap
            .stepContent__pandaicon__menu__list--img
              width: 160px
              margin: 32px auto 0


    .stepContent__button
      width: 100%
      margin: 0 auto 
    .stepContent__button__wrap
      +pc()
        width: 600px
        margin: 0 auto
      +sp()
        width: calc(100% - 32px)
        margin: 0 auto
      .stepContent__button__icon
        margin: 0 auto
        +pc()
          width: 222px
          margin-top :32px
        +sp()
          width: 137px
          margin-top :24px
      .stepContent__button--link
        display: inline-block
        margin: auto
        background: followBtnColor
        border-radius: 44px
        color: whiteColor
        &.-blue
          background: blueColor
        +pc()
          width: 600px
          padding: 20px 0
          font-size: 3.6rem
          line-height: 1.3
        +sp()
          width: calc(100% - 61px)
          padding: 16px 0
          font-size: 1.8rem
          line-height: 1.5
        &:hover
          opacity: .8
          color: whiteColor
          transition-duration: 0.15s

        // //終了用ボタン
        // &.endButton
        //   background-color: grayTagColor
        //   color: white
        //   pointer-events: none
    .stepContent__share__long
      +pc()
        width: 249px
        margin: 40px auto 0
      +sp()
        width: calc(100% - 122px)
        margin: 40px auto 0
    .stepContent__info
      text-align: center
      line-height: 1.8
      +pc()
        margin-top: 60px
        font-size: 1.4rem
      +sp()
        width: calc(100% - 32px)
        margin: 40px auto 0
        font-size: 1.2rem
        text-align: left
      .stepContent__info--list
        text-align: center
        +pc()
          &:nth-of-type(n+2)
            display: inline-block
        .stepContent__info--link
          color: secondColor
          &:hover
            opacity: .8
            transition-duration: 0.15s
    .stepContent__post
      +pc()
        width: 600px
        margin: 60px auto 0
      +sp()
        width: calc(100% - 32px)
        margin: 40px auto 0
      .stepContent__post--title
        display: inline-block
        position: relative
        line-height: 1.4
        font-size: 2.8rem
        .stepContent__post--title--text
          display: inline-block
          position: relative
          font-weight: bold
          line-height: 1.4
          +pc()
            font-size: 3.6rem
          +sp()
            font-size: 2.8rem
          &::before
            content: "|"
            position: absolute
            top: 0
            font-weight: normal
            transform: rotate(-45deg)
            +pc()
              top: 0.5rem
              left: -2.5rem
              font-size: 2.8rem
            +sp()
              top: 0.5rem
              left: -2rem
              font-size: 2rem
          &::after
            content: "|"
            position: absolute
            top: 0
            font-weight: normal
            transform: rotate(45deg)
            +pc()
              top: 0.5rem
              right: -2.5rem
              font-size: 2.8rem
            +sp()
              top: 0.5rem
              right: -2rem
              font-size: 2rem
        .stepContent__post--title--img
          display: block
          margin: 0 auto
          +pc()
            width: 400px
            padding-top: 14px
          +sp()
            width: 300px
            padding-top: 8px
        .stepContent__post--title--info
          margin-top: 8px
          font-size: 1.4rem
          +pc()
            line-height: 1.5
          +sp()
            line-height: 1.4

      .stepContent__post__summary
        margin-top: 40px
        .stepContent__post__summary--text
          font-weight: bold
          line-height: 1.4
          font-size: 2rem
          .stepContent__post__summary--emphasis
            display: inline-block
            padding-left: 2px
            color: followBtnColor
            .stepContent__post__summary--emphasis-point
              letter-spacing: 1px
              +pc()
                font-size: 3.8rem
              +sp()
                font-size: 3rem
        .stepContent__post__summary--info
          margin-top: 8px
          +pc()
            font-size: 1.4rem
            line-height: 1.5
          +sp()
            font-size: 1.2rem
            line-height: 1.4
      .stepContent__post__container
        margin-top: 40px
        .stepContent__post__menu
          display: flex
          justify-content: center
          flex-direction: column
          +pc()
            align-items: flex-end
          +sp()
            margin: auto
          .stepContent__post__menu__list
            width: 100%
            &:first-of-type
              +pc()
                margin-bottom: 56px
              +sp()
                margin-bottom: 40px
            .stepContent__post__menu__list--text__wrap
              display: flex
              justify-content: center
              font-size: 1.8rem
              font-weight: bold
              line-height: 1.4
              .stepContent__post__menu__list--text--icon
                display: flex
                justify-content: center
                align-items: center
                position: relative
                top: 4px
                width: 18px
                height: 18px
                background: #333333
                box-sizing: border-box
                border-radius: 18px
                color: backmainColor
                font-size: 1.2rem
                font-weight: bold
                line-height: 1.9
                &.-second
                  left:32px
