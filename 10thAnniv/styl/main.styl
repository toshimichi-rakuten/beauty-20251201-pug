                +pc()
                  margin-top: 16px
                +sp()
                  margin-top: 8px
            .stepContent__post__menu__list--img
              display: block
              +pc()
                width: 260px
                margin: 28px auto 0
              +sp()
                width: calc(100% - 94px)
                margin: 16px auto 0
              &.-long
                +pc()
                  margin: 16px auto 0
              &.-min
                +pc()
                  margin: 16px auto 0                  
      .stepContent__post__contents
        background: whiteColor
        box-sizing: border-box
        +pc()
          width: 450px
          margin: 60px auto 0
          padding: 40px
        +sp()
          width: 90%
          margin: 40px auto 0
          padding: 40px 24px
        .stepContent__post__contents--title
          +pc()
            width: 281px
            margin: auto
          +sp()
            width: 58.6%
            margin: auto
        .stepContent__post__contents--info
          line-height: 1.4
          font-weight: bold
          +pc()
            margin-top: 40px
            font-size: 1.8rem
            font-weight: bold
          +sp()
            margin-top: 24px
            font-size: 1.6rem
        .stepContent__post__contents__list
          display: flex
          justify-content: center
          width: 100%
          +pc()
            margin: 40px auto 0
          +sp()
            margin: 24px auto 0
          .stepContent__post__contents__list__img
            width: 100%
    .stepContent--result
