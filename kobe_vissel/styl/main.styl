 // Search Area
  .searchArea
    +pc()
      display: block
      padding-bottom: 70px
      margin-bottom: 60px
    +sp()
      margin-bottom: 40px
    .searchArea--container
      position: relative
      margin: 0 auto
      +pc()
        width: 700px
      +sp()
        width: calc(100% - 41px)
        padding-bottom: 4px
      .searchArea--desc--icon
        position: absolute
        z-index: 5
        top: 0
        pointer-events: none
        +pc()
          right: 23px
          height: 208px
        +sp()
          right: 0
          height: 98px
        img
          height 100%
      .searchArea--button
        display: inline-block
        background: crimsonRedColor
        border-radius: 44px
        color: #fff
        text-align: center
        +pc()
          width: 600px
          padding: 20px 0
          font-size: 3.6rem
          line-height: 1.3
        +sp()
          width: 88%
          padding: 16px 0
          font-size: 2rem
          line-height: 1.5
        &:hover
          opacity: .8
      .searchArea--info
        text-align: center
        +pc()
          width: 600px
          padding-top: 24px
          font-size: 2rem
          font-weight: bold
          line-height: 1.4
        +sp()
          width: 88%
          padding-top: 12px
          font-size: 1.2rem
          line-height: 1.8

    // Search Area Component / ()内に色を入れる
    @import passStr + '_common/stylus/Module/Common/_searchArea'
    searchArea(mainColor)

  
  // app DL
  .app
    +pc()
      margin-bottom: 60px
    +sp()
      margin-bottom: 40px

    // app DL parts(bgColor)
    // 目立たせたい時は #FFE8E8 など推奨
    appDL(#FFE8E8)

  // banner
  .banner
    margin: 0 auto 60px
    +sp()
      margin-bottom: 40px
    .banner__box
      text-align: center
      background-color: grayBgColor
      box-sizing: border-box
      +pc()
        width: 890px
        margin: 0 auto
        padding: 24px
      +sp()
        width: auto
        margin: 0 8px
        padding: 10px 16px 16px
      .banner__txt
        font-weight: bold
        line-height: 1.5
        +pc()
          font-size 2rem
          padding-bottom: 24px
        +sp()
          font-size 1.4rem
          padding-bottom: 8px
      .banner__boxInner
        background-color: #fff
        box-sizing: border-box
        +pc()
          padding: 24px
        +sp()
          padding: 16px
        .banner__link
          display: block
          width: auto
          max-width: 640px
          margin: 0 auto
          transition: opacity .2s ease-out
          &:hover
            opacity: 0.8
          .banner__img
            width: 100%
            height: auto


