  //- Breadcrumb for SP
  -
   var linklist = lists

  #SPbreadcrumb.c-container.-sp
    ul(class="c-breadcrumbs")
      li.c-breadcrumbs__item: a(href = "/", class="c-breadcrumbs__link") 楽天ビューティ

      each item in linklist
        if item.link !== 'current'
          li.c-breadcrumbs__item: a(href = item.link, class="c-breadcrumbs__link") #{item.name}
        else
          li.c-breadcrumbs__item: strong.c-breadcrumbs__current #{item.name}


  
